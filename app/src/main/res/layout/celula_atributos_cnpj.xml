<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/iconesAtributos"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/bordas_100_solid_blue400"
        android:backgroundTint="@color/white"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tituloIcones"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:text="-"
        android:fontFamily="@font/roboto"
        android:textColor="@color/gray500"
        android:textSize="14sp"
        android:textFontWeight="400"
        android:letterSpacing="0.015"

        app:layout_constraintBottom_toBottomOf="@+id/iconesAtributos"
        app:layout_constraintStart_toEndOf="@+id/iconesAtributos"
        app:layout_constraintTop_toTopOf="@+id/iconesAtributos" />
</androidx.constraintlayout.widget.ConstraintLayout>